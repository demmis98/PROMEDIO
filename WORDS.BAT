@ECHO OFF
SETLOCAL EnableDelayedExpansion
SET/A PRES=10
SET 1=A
SET 2=B
SET 3=C
SET 4=D
SET 5=E
SET 6=F
SET 7=G
SET 8=H
SET 9=I
SET 10=J
SET 11=K
SET 12=L
SET 13=M
SET 14=N
SET 15=O
SET 16=P
SET 17=Q
SET 18=R
SET 19=S
SET 20=T
SET 21=U
SET 22=V
SET 23=W
SET 24=X
SET 25=Y
SET CANT=1
:LOOP
IF %PRES%==%CANT% GOTO NEXT
SET/A RAN=%RANDOM%*26/32768
IF %RAN%==0 GOTO CERO
IF DEFINED WORD ECHO %WORD%
GOTO WORD
:WORD
IF DEFINED WORD%CANT% GOTO WORD1
GOTO WORD0
:WORD0
SET WORD%CANT%=!%RAN%!
SET WORD=!WORD%CANT%!
GOTO LOOP
:WORD1
SET WORD%CANT%=!WORD%CANT%!!%RAN%!
SET WORD=!WORD%CANT%!
GOTO LOOP
:CERO
IF DEFINED WORD%CANT% GOTO CERO1
GOTO LOOP
:CERO1
IF DEFINED !WORD%CANT%! GOTO CANT1
GOTO CANT0
:CANT0
SET/A !WORD%CANT%!=0
SET/A CANT=%CANT%+1
GOTO LOOP
:CANT1
SET WORDz=!WORD%CANT%!
SET/A !WORD%CANT%!=!%WORD%!+1
SET/A CANT=%CANT%+1
GOTO LOOP
:NEXT
SET/A CANT2=%CANT%
SET/A CANT=0
:LOOP2
SET ORDER%CANT%=!WORD%CANT%!
IF %CANT%==%CANT2% GOTO ORDER
SET/A CANT=%CANT%+1
GOTO LOOP2
:ORDER
SET/A CANTER=0
SET/A CANTERER=0
SET/A CANT=0
:ORDER1
SET/A CANTER=%CANTER%+1
IF %CANTER%==%CANT2% GOTO NEXTER
:ORDER2
SET CANT1=%CANT+1%
SET WORD=!ORDER%CANT%!
SET WORD=!%WORD%!
SET WORD1=!ORDER%CANT1%!
SET WORD1=!%WORD1%!
IF !%WORD%! LSS !%WORD1%! GOTO LESS
GOTO ORDER1
:LESS
SET WORD=!ORDER%CANT%!
SET WORD1=!ORDER%CANT1%!
SET ORDER%CANT%=%WORD1%
SET ORDER%CANT1%=%WORD%
GOTO ORDER1
:NEXTER
SET/A CANT=0
:NEXTERER
SET/A CANT=%CANT%+1
SET WORD=!WORD%CANT%!
SET WORD=!%WORD%!
SET WORDER=!WORD%CANT%!
ECHO %WORDER% %WORD%
PAUSE>NUL